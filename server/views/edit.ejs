<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/header.ejs') %>
  <body>
    <%- include('./partials/nav.ejs') %>

    <form action="" method="POST" id="form">
      <label for="name">Name</label><br />
      <input
        type="text"
        name="name"
        id="name"
        value="<%= contact.name %>"
        required
      /><br /><br />
      <label for="phone">Phone number</label><br />
      <input
        type="text"
        name="phone"
        id="phone"
        value="<%= contact.phone %>"
        required
      /><br /><br />
      <label for="email">Email</label><br />
      <input
        type="text"
        name="email"
        id="email"
        value="<%= contact.email %>"
      /><br /><br />
      <input type="submit" name="" id="" class="btn" value="Edit Contact" />
      <a href="/business_contact">Cancel</a>
    </form>
  </body>
</html>
